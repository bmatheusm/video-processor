{
  "family": "video-processor-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::395368505712:role/LabRole",
  "containerDefinitions": [
    {
      "name": "video-processor",
      "image": "REPLACE_IMAGE",
      "essential": true,
      "environment": [
