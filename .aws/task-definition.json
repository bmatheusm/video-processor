          "value": "jdbc:postgresql://database-1.cluster-crzfgagwhurg.us-east-1.rds.amazonaws.com:5432/postgres"
        },
        {
          "name": "POSTGRES_USER",
          "value": "postgres"
        },
        {
          "name": "S3_BUCKET",
          "value": "videos-hackaton"
        },
        {
          "name": "EMAIL_REMETENTE",
          "value": "hackatonfiap28@gmail.com"
        }
      ],
      "secrets": [
        {
          "name": "POSTGRES_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:395368505712:secret:POSTGRES_PASSWORD-1f1ddk"
        },
        {
          "name": "USER_EMAIL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:395368505712:secret:USER_EMAIL-4cCc3e"
        },
        {
          "name": "PASSWORD_EMAIL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:395368505712:secret:PASSWORD_EMAIL-y3BGtJ"
        },
        {
          "name": "AWS_ACCESS_KEY_ID",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:395368505712:secret:AWS_ACCESS_KEY_ID-ZFRvxv"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:395368505712:secret:AWS_SECRET_ACCESS_KEY-G6FL7j"
        },
        {
          "name": "AWS_SESSION_TOKEN",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:395368505712:secret:AWS_SESSION_TOKEN-v7Q2wx"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
